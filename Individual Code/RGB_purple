const int PIN_RED   = 8;
const int PIN_GREEN = 9;
const int PIN_BLUE  = 10;

void setup() {
  pinMode(PIN_RED,   OUTPUT);
  pinMode(PIN_GREEN, OUTPUT);
  pinMode(PIN_BLUE,  OUTPUT);
}

void purple(){
  
    analogWrite(PIN_RED,   153);
    analogWrite(PIN_GREEN, 255);
    analogWrite(PIN_BLUE,  0);
    delay(1000);
  }

  void loop() {
  
  purple();
  
  
  
  }
